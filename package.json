{
  "name": "ouafouaf",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:setup": "pnpm --filter @repo/database db:setup",
    "db:migrate": "pnpm --filter @repo/database db:migrate",
    "db:reset": "pnpm --filter @repo/database db:reset",
    "db:test:setup": "pnpm --filter @repo/database db:test:setup",
    "db:test:migrate": "pnpm --filter @repo/database db:test:migrate",
    "db:test:reset": "pnpm --filter @repo/database db:test:reset",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.2",
    "turbo": "^1.13.4",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}